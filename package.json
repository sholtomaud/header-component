{
  "name": "header-component",
  "description": "Header component for app built with fastn",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "watch": "watchify scripts/index.js -o client/index.browser.js -d -v & stylus -u nib -w stylus/index.styl -o client/styles/index.css",
    "dist": "npm version patch -m \"Bumped to %s\"",
    "postversion": "git push && git push --tags && npm run build",
    "test": "test",
    "docuBuild": "docu build --type client --os centos7 --node 6 --dockerfile Dockerfile",
    "docuRun": "docu run --type client --node 6",
    "build": "browserify -t brfs example/index.js >  example/index.browser.js ",
    "example:js": "browserify -t brfs example/index.js >  example/index.browser.js ",
    "build:example": "npm-run-all example:*"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Sholto Maud",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "dion": "^1.0.2",
    "nodemon": "^1.9.2",
    "sea-lion": "^1.0.5"
  },
  "devDependencies": {
    "activity-router": "^1.3.1",
    "browserify": "^13.0.0",
    "debug": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-google": "^0.5.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "fastn": "^2.7.2",
    "minifyify": "^7.3.3",
    "rootify": "^1.1.0",
    "rooty": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1"
  },
  "browserify": {
    "transform": [
      "rootify"
    ]
  },
  "rootify": {
    "rootPath": "./scripts"
  },
  "version": "1.0.0",
  "keywords": [
    "fastn",
    "header",
    "app"
  ]
}
